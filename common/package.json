{
  "name": "@sketch-bridge/common",
  "version": "1.0.0",
  "description": "A common folder for sharing code between cheza-cheza clients and APIs",
  "type": "module",
  "main": "./dist/model/index.js",
  "types": "./dist/model/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.js"
    },
    "./model": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.js"
    },
    "./model/*": {
      "types": "./dist/model/*.d.ts",
      "import": "./dist/model/*.js"
    }
  },
  "scripts": {
    "build": "tsc || true",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Common",
    "folder",
    "shared",
    "interfaces"
  ],
  "author": "Akida Software",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "private": true
}
